                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DI.SK_NIV_NivelAnalisis, SK_CLI_Cliente, SK_SCL_Segmentacion, O.SK_OFC_CODIGO, SK_FAM_FamiliaHistorico, SK_SUC_Sucursal, NU_Cantidad
FROM 
		(SELECT 
			cast(2 as smallint) as SK_NIV_NivelAnalisis,
			-1 AS SK_CLI_Cliente,
			(SELECT SK_SCL_SEGMENTACION FROM LK_SegmentacionClientes WHERE VC_SCL_CODIGO = 'PR') as SK_SCL_Segmentacion,
			CAST(-1 AS bigint) AS SK_FAM_FamiliaHistorico,
			CAST(-1 AS bigint) AS SK_SUC_Sucursal,
			SUM(NU_CantidadMercado) as NU_Cantidad
		
		FROM BT_DIMENSIONAMIENTO D
		WHERE
			EXISTS(SELECT SK_ACE_CODIGO 
					FROM LK_Cliente C 
					WHERE 
						EXISTS(SELECT 1 
						 	   FROM BT_Facturacion F 
						 	   WHERE 
									F.SK_CLI_Cliente = C.SK_CLI_Cliente 
						 	   		AND SK_OFC_Codigo IN (1,2)
								) 
						AND D.SK_ACE_Codigo = C.SK_ACE_Codigo)
			AND SK_Mes = ?
		) DI 
	CROSS JOIN (select sk_ofc_codigo 
				from LK_OrigenFacturacion 
				where sk_ofc_codigo in (1,2)) o</property>
