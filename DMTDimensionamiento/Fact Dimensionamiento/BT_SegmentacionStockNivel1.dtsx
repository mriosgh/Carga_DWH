    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{B463D5E9-1C34-423C-BED9-0D4EE7A4D5B1}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT &#xA;&#x9;cast(1 as smallint) as SK_NIV_NivelAnalisis,&#xA;&#x9;SK_CLI_Cliente,&#xA;&#x9;IIF(DATEDIFF(MM,FP_FACT_Venta,'&quot; + @[User::FechaTopeAnalisis]  + &quot;') &lt;= 12*&quot; + (DT_WSTR,1)@[User::AñosVentas] + &quot; OR DATEDIFF(MM,FP_FACT_Alquiler,'&quot; + @[User::FechaTopeAnalisis]  + &quot;') &lt;= 12*&quot; + (DT_WSTR,1)@[User::AñosAlquileres] + &quot; , (SELECT SK_SCL_SEGMENTACION FROM LK_SegmentacionClientes WHERE VC_SCL_CODIGO = 'CL'), (SELECT SK_SCL_SEGMENTACION FROM LK_SegmentacionClientes WHERE VC_SCL_CODIGO = 'PA')) as SK_SCL_Segmentacion,&#xA;&#x9;CAST(-1 AS bigint) AS SK_OFC_Codigo,&#xA;&#x9;CAST(-1 AS bigint) AS SK_FAM_FamiliaHistorico,&#xA;&#x9;CAST(-1 AS bigint) AS SK_SUC_Sucursal,&#xA;&#x9;CAST(1 as bigint) as NU_Cantidad&#xA;&#xA;FROM BT_Clientes C&#xA;WHERE&#xA;&#x9;EXISTS(SELECT 1 FROM BT_Facturacion F WHERE F.SK_CLI_Cliente = C.SK_CLI_Cliente AND SK_OFC_Codigo IN (1,2))&#xA;&#x9;AND SK_Mes =  &quot; + (DT_WSTR,6) @[User::MesCorrida] + &quot;&#xA;UNION&#xA;SELECT &#xA;&#x9;cast(1 as smallint) as SK_NIV_NivelAnalisis,&#xA;&#x9;-1 AS SK_CLI_Cliente,&#xA;&#x9;(SELECT SK_SCL_SEGMENTACION FROM LK_SegmentacionClientes WHERE VC_SCL_CODIGO = 'PR') as SK_SCL_Segmentacion,&#xA;&#x9;CAST(-1 AS bigint) AS SK_OFC_Codigo,&#xA;&#x9;CAST(-1 AS bigint) AS SK_FAM_FamiliaHistorico,&#xA;&#x9;CAST(-1 AS bigint) AS SK_SUC_Sucursal,&#xA;&#x9;SUM(NU_CantidadPotenciales) as NU_Cantidad&#xA;&#xA;FROM BT_DIMENSIONAMIENTO D&#xA;WHERE&#xA;&#x9;EXISTS(SELECT SK_ACE_CODIGO FROM LK_Cliente C WHERE EXISTS(SELECT 1 FROM BT_Facturacion F WHERE F.SK_CLI_Cliente = C.SK_CLI_Cliente AND SK_OFC_Codigo IN (1,2)) AND D.SK_ACE_Codigo = C.SK_ACE_Codigo)&#xA;&#x9;AND SK_Mes =  &quot; + (DT_WSTR,6) @[User::MesCorrida]"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="QryExtractNivel1">
      <DTS:VariableValue
        DTS:DataType="8">SELECT 
	cast(1 as smallint) as SK_NIV_NivelAnalisis,
	SK_CLI_Cliente,
	IIF(DATEDIFF(MM,FP_FACT_Venta,'20191201') &lt;= 12*5 OR DATEDIFF(MM,FP_FACT_Alquiler,'20191201') &lt;= 12*1 , (SELECT SK_SCL_SEGMENTACION FROM LK_SegmentacionClientes WHERE VC_SCL_CODIGO = 'CL'), (SELECT SK_SCL_SEGMENTACION FROM LK_SegmentacionClientes WHERE VC_SCL_CODIGO = 'PA')) as SK_SCL_Segmentacion,
	CAST(-1 AS bigint) AS SK_OFC_Codigo,
	CAST(-1 AS bigint) AS SK_FAM_FamiliaHistorico,
	CAST(-1 AS bigint) AS SK_SUC_Sucursal,
	CAST(1 as bigint) as NU_Cantidad

FROM BT_Clientes C
WHERE
	EXISTS(SELECT 1 FROM BT_Facturacion F WHERE F.SK_CLI_Cliente = C.SK_CLI_Cliente AND SK_OFC_Codigo IN (1,2))
	AND SK_Mes =  201912
UNION
SELECT 
	cast(1 as smallint) as SK_NIV_NivelAnalisis,
	-1 AS SK_CLI_Cliente,
	(SELECT SK_SCL_SEGMENTACION FROM LK_SegmentacionClientes WHERE VC_SCL_CODIGO = 'PR') as SK_SCL_Segmentacion,
	CAST(-1 AS bigint) AS SK_OFC_Codigo,
	CAST(-1 AS bigint) AS SK_FAM_FamiliaHistorico,
	CAST(-1 AS bigint) AS SK_SUC_Sucursal,
	SUM(NU_CantidadPotenciales) as NU_Cantidad

FROM BT_DIMENSIONAMIENTO D
WHERE
	EXISTS(SELECT SK_ACE_CODIGO FROM LK_Cliente C WHERE EXISTS(SELECT 1 FROM BT_Facturacion F WHERE F.SK_CLI_Cliente = C.SK_CLI_Cliente AND SK_OFC_Codigo IN (1,2)) AND D.SK_ACE_Codigo = C.SK_ACE_Codigo)
	AND SK_Mes =  201912</DTS:VariableValue>
    </DTS:Variable>
    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{F581B1BA-78CF-4FF9-863F-1995697DBC16}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT &#xA;&#x9;cast(2 as smallint) as SK_NIV_NivelAnalisis,&#xA;&#x9;SK_CLI_Cliente,&#xA;&#x9;IIF((DATEDIFF(MM,iif(sk_ofc_codigo = 1,FP_FACT_Alquiler,FP_FACT_Venta), '&quot; + @[User::FechaTopeAnalisis]  + &quot;') &lt;= 12*iif(sk_ofc_codigo = 1, &quot; + (DT_WSTR,1)@[User::AñosAlquileres] + &quot;,&quot; +(DT_WSTR,1) @[User::AñosVentas] + &quot;)),(SELECT SK_SCL_SEGMENTACION FROM LK_SegmentacionClientes WHERE VC_SCL_CODIGO = 'CL') , iif(iif(sk_ofc_codigo = 1,FP_FACT_Alquiler,FP_FACT_Venta) &gt; '1970-01-01',(SELECT SK_SCL_SEGMENTACION FROM LK_SegmentacionClientes WHERE VC_SCL_CODIGO = 'PA') ,iif(iif(sk_ofc_codigo = 1,FP_FACT_Venta,FP_FACT_Alquiler) &gt; '1970-01-01',(SELECT SK_SCL_SEGMENTACION FROM LK_SegmentacionClientes WHERE VC_SCL_CODIGO = 'CN'),(SELECT SK_SCL_SEGMENTACION FROM LK_SegmentacionClientes WHERE VC_SCL_CODIGO = 'PR')))) as SK_SCL_Segmentacion,&#xA;&#x9;SK_OFC_Codigo,&#xA;&#x9;CAST(-1 AS bigint) AS SK_FAM_FamiliaHistorico,&#xA;&#x9;CAST(-1 AS bigint) AS SK_SUC_Sucursal,&#xA;&#x9;CAST(1 as bigint) as NU_Cantidad&#xA;FROM BT_Clientes C&#xA;&#x9;cross join (select sk_ofc_codigo from LK_OrigenFacturacion where sk_ofc_codigo in (1,2)) o&#xA;WHERE&#xA;&#x9;EXISTS(SELECT 1 FROM BT_Facturacion F WHERE F.SK_CLI_Cliente = C.SK_CLI_Cliente AND SK_OFC_Codigo IN (1,2) AND FC_Factura &lt;= '&quot; + @[User::FechaTopeAnalisis] + &quot;')&#xA;&#x9;AND SK_Mes = &quot; + (DT_WSTR,6) @[User::MesCorrida]"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="QryExtractNivel2New">
      <DTS:VariableValue
        DTS:DataType="8">SELECT 
	cast(2 as smallint) as SK_NIV_NivelAnalisis,
	SK_CLI_Cliente,
	IIF((DATEDIFF(MM,iif(sk_ofc_codigo = 1,FP_FACT_Alquiler,FP_FACT_Venta), '20191201') &lt;= 12*iif(sk_ofc_codigo = 1, 1,5)),(SELECT SK_SCL_SEGMENTACION FROM LK_SegmentacionClientes WHERE VC_SCL_CODIGO = 'CL') , iif(iif(sk_ofc_codigo = 1,FP_FACT_Alquiler,FP_FACT_Venta) &gt; '1970-01-01',(SELECT SK_SCL_SEGMENTACION FROM LK_SegmentacionClientes WHERE VC_SCL_CODIGO = 'PA') ,iif(iif(sk_ofc_codigo = 1,FP_FACT_Venta,FP_FACT_Alquiler) &gt; '1970-01-01',(SELECT SK_SCL_SEGMENTACION FROM LK_SegmentacionClientes WHERE VC_SCL_CODIGO = 'CN'),(SELECT SK_SCL_SEGMENTACION FROM LK_SegmentacionClientes WHERE VC_SCL_CODIGO = 'PR')))) as SK_SCL_Segmentacion,
	SK_OFC_Codigo,
	CAST(-1 AS bigint) AS SK_FAM_FamiliaHistorico,
	CAST(-1 AS bigint) AS SK_SUC_Sucursal,
	CAST(1 as bigint) as NU_Cantidad
FROM BT_Clientes C
	cross join (select sk_ofc_codigo from LK_OrigenFacturacion where sk_ofc_codigo in (1,2)) o
WHERE
	EXISTS(SELECT 1 FROM BT_Facturacion F WHERE F.SK_CLI_Cliente = C.SK_CLI_Cliente AND SK_OFC_Codigo IN (1,2) AND FC_Factura &lt;= '20191201')
	AND SK_Mes = 201912</DTS:VariableValue>
    </DTS:Variable>
