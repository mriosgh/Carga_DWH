                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT	LTRIM(RTRIM(LILOTN)) as LILOTN,-- nroDeSerie
		CAST(LTRIM(RTRIM(LIITM)) AS BIGINT) AS LIITM, -- id item origen
		LTRIM(RTRIM(IMLITM)) AS IMLITM, --2do item origen
		LTRIM(RTRIM(LIMCU)) AS LIMCU, -- unidad de negocio
		LTRIM(RTRIM(IMSRP3)) AS IMSRP3, -- codigoModelo
		LTRIM(RTRIM(LILOCN)) AS LILOCN,-- ubicacionAlmacenamiento 
		LTRIM(RTRIM(LIGLPT)) AS LIGLPT,-- categoria
		LTRIM(RTRIM(LIPQOH)) AS LIPQOH,-- cantidad
		CAST(DATEADD(dd,((LTRIM(RTRIM(LILRCJ)) % 1000) - 1),(cast(cast(1900+(LTRIM(RTRIM(LILRCJ))/1000) as decimal(4,0)) as varchar(4)) + '01' + '01')) AS DATE)as fechaUltMovimiento,
		DATEADD(dd,((LTRIM(RTRIM(LIUPMJ)) % 1000) - 1),(cast(cast(1900+(LTRIM(RTRIM(LIUPMJ))/1000) as decimal(4,0)) as varchar(4)) + '01' + '01')) as fechaActualizacion
FROM [PRODDTA].[F41021] A 
LEFT JOIN [PRODDTA].[F4101] B
ON LTRIM(RTRIM(CAST(A.LIITM AS CHAR))) = LTRIM(RTRIM(B.IMITM))
WHERE 
	LIPQOH &gt; 0	--CANTIDAD &gt; 0</property>
