ON LTRIM(RTRIM(CAST(A.LIITM AS CHAR))) = LTRIM(RTRIM(B.IMITM))

WHERE LTRIM(RTRIM(LIMCU)) IN ('901','201','901G','201G')

AND LTRIM(RTRIM(IMPRP2)) LIKE 'E'

AND LTRIM(RTRIM(IMSRNR)) = 'Y'

AND LTRIM(RTRIM(LIGLPT)) IN ('IN20','IN21','IN28')

AND LIPQOH &gt; 0
AND (LTRIM(RTRIM(LILOTN)) IS NOT NULL
