                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT	PDDOCO AS numeroOrdenCompra,
		PDLNID AS numeroLinea,
		LTRIM(RTRIM(PDSFXO)) AS numeroSufijoOrden,
		LTRIM(RTRIM(PDDCTO)) AS tipoOrden,
		LTRIM(RTRIM(PDMCU)) AS unidadNegocio,
		LTRIM(RTRIM(PDLOTN)) AS numeroSerie,
		LTRIM(RTRIM(PDVR02)) AS numeroFactura,
		PDAN8 AS numeroProveedor,
		LTRIM(RTRIM(PDUOM)) AS unidadMedidaCompra,
		PDUORG AS cantidadEnviada,
		PDITM AS numeroArticulo,
		LTRIM(RTRIM(PDLTTR)) AS codigoEstadoUltimo,
		LTRIM(RTRIM(PDNXTR)) AS codigoEstadoSiguiente,
		PDPRRC AS costoUnitario,
		PDAEXP AS precioTotalPesos,
		DATEADD(dd,((LTRIM(RTRIM(PDDRQJ)) % 1000) - 1),(cast(cast(1900+(LTRIM(RTRIM(PDDRQJ))/1000) as decimal(4,0)) as varchar(4)) + '01' + '01')) as fechaSolicitud,
		DATEADD(dd,((LTRIM(RTRIM(PDTRDJ)) % 1000) - 1),(cast(cast(1900+(LTRIM(RTRIM(PDTRDJ))/1000) as decimal(4,0)) as varchar(4)) + '01' + '01')) as fechaOrden,
		DATEADD(dd,((LTRIM(RTRIM(PDCNDJ)) % 1000) - 1),(cast(cast(1900+(LTRIM(RTRIM(PDCNDJ))/1000) as decimal(4,0)) as varchar(4)) + '01' + '01')) as fechaCancelacion,
		DATEADD(dd,((LTRIM(RTRIM(PDOPDJ)) % 1000) - 1),(cast(cast(1900+(LTRIM(RTRIM(PDOPDJ))/1000) as decimal(4,0)) as varchar(4)) + '01' + '01')) as fechaEntregaPrometida,
		DATEADD(dd,((LTRIM(RTRIM(PDPSDJ)) % 1000) - 1),(cast(cast(1900+(LTRIM(RTRIM(PDPSDJ))/1000) as decimal(4,0)) as varchar(4)) + '01' + '01')) as fechaEnvioPrometida,
		DATEADD(dd,((LTRIM(RTRIM(PDURDT)) % 1000) - 1),(cast(cast(1900+(LTRIM(RTRIM(PDURDT))/1000) as decimal(4,0)) as varchar(4)) + '01' + '01')) as fechaEntrega,
		DATEADD(dd,((LTRIM(RTRIM(PDUPMJ)) % 1000) - 1),(cast(cast(1900+(LTRIM(RTRIM(PDUPMJ))/1000) as decimal(4,0)) as varchar(4)) + '01' + '01')) as fechaActualizacion
FROM [PRODDTA].[F4311]
WHERE (LTRIM(RTRIM(PDMCU)) LIKE '901%'
OR LTRIM(RTRIM(PDMCU)) LIKE '201%'
OR LTRIM(RTRIM(PDMCU)) LIKE '458%'
OR LTRIM(RTRIM(PDMCU)) LIKE '452%'
OR LTRIM(RTRIM(PDMCU)) LIKE '904%'
OR LTRIM(RTRIM(PDMCU)) LIKE '302%');</property>
