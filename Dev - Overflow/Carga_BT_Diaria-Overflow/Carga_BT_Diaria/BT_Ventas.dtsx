      DTS:ObjectName="QryBTVentas">
      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION

MERGE INTO BT_Ventas VEN
USING (
SELECT * FROM (
SELECT 
		(ROW_NUMBER() OVER(PARTITION BY numeroOrdenVenta, numeroLinea, tipoOrden, numeroOrdenCompania ORDER BY codigoEstadoSiguiente desc)) AS agrupado
		,[ID_VentasEmitidas]
		,CASE WHEN U.SK_UNI_UnidadNegocio IS NULL THEN -1 ELSE U.SK_UNI_UnidadNegocio END AS SK_UNI_UnidadNegocio
		,CASE WHEN A.SK_ART_Articulo IS NULL THEN -1 ELSE A.SK_ART_Articulo END AS SK_ART_Articulo
		,CASE WHEN [numeroOrdenCompania] IS NULL THEN '*sin dato*' ELSE [numeroOrdenCompania] END AS NU_Orden_Compania
		,CASE WHEN [numeroOrdenVenta] IS NULL THEN -1 ELSE [numeroOrdenVenta] END AS [numeroOrdenVenta]
		,CASE WHEN [numeroLinea] IS NULL THEN -1 ELSE [numeroLinea] END AS [numeroLinea]
		,CASE WHEN LTRIM(RTRIM([numeroSufijoOrden])) IS NULL OR LTRIM(RTRIM([numeroSufijoOrden])) LIKE '' THEN '-1' ELSE LTRIM(RTRIM([numeroSufijoOrden])) END AS [numeroSufijoOrden]
		,CASE WHEN D.SK_TDO_Tipo_Documento IS NULL THEN -1 ELSE D.SK_TDO_Tipo_Documento END AS SK_TDO_Tipo_Documento
		,CASE WHEN LTRIM(RTRIM([numeroSerie])) IS NULL OR LTRIM(RTRIM([numeroSerie])) LIKE '' THEN '*sin dato*' ELSE LTRIM(RTRIM([numeroSerie])) END AS [numeroSerie]
		,CASE WHEN LTRIM(RTRIM([numeroFactura])) IS NULL OR LTRIM(RTRIM([numeroFactura])) LIKE '' THEN '*sin dato*' ELSE LTRIM(RTRIM([numeroFactura])) END AS [numeroFactura]
		,CASE WHEN C.SK_CLI_Cliente IS NULL THEN -1 ELSE C.SK_CLI_Cliente END AS SK_CLI_Cliente
		,CASE WHEN CAST(LTRIM(RTRIM([numeroOrdenVentaOriginal])) AS INT) IS NULL OR CAST(LTRIM(RTRIM([numeroOrdenVentaOriginal])) AS INT) = 0 THEN -1 ELSE CAST(LTRIM(RTRIM([numeroOrdenVentaOriginal])) AS INT) END AS [numeroOrdenVentaOriginal]
		,CASE WHEN D.SK_TDO_Tipo_Documento IS NULL THEN -1 ELSE D.SK_TDO_Tipo_Documento END AS SK_TDO_Tipo_Documento_Original		
		,CASE WHEN LTRIM(RTRIM([codigoEstadoUltimo])) IS NULL OR LTRIM(RTRIM([codigoEstadoUltimo])) LIKE '' THEN '-1' ELSE LTRIM(RTRIM([codigoEstadoUltimo])) END AS [codigoEstadoUltimo]
		,CASE WHEN LTRIM(RTRIM([codigoEstadoSiguiente])) IS NULL OR LTRIM(RTRIM([codigoEstadoSiguiente])) LIKE '' THEN '-1' ELSE LTRIM(RTRIM([codigoEstadoSiguiente])) END AS [codigoEstadoSiguiente]
		,CASE WHEN LTRIM(RTRIM([unidadMedidaVenta])) IS NULL OR LTRIM(RTRIM([unidadMedidaVenta])) LIKE '' THEN '*sin dato*' ELSE LTRIM(RTRIM([unidadMedidaVenta])) END AS [unidadMedidaVenta]
		,CASE WHEN [cantidadEnviada] IS NULL THEN 0 ELSE [cantidadEnviada] END AS [cantidadEnviada]
		,CASE WHEN [costoUnitario] IS NULL THEN 0 ELSE [costoUnitario] END AS [costoUnitario]
		,CASE WHEN [precioTotalPesos] IS NULL THEN 0 ELSE [precioTotalPesos] END AS [precioTotalPesos]
		,CASE WHEN [fechaRequerido] IS NULL THEN '1/1/1970 12:00:00' ELSE LTRIM(RTRIM([fechaRequerido])) END AS [fechaRequerido]
		,CASE WHEN [fechaOrden] IS NULL THEN '1/1/1970 12:00:00' ELSE LTRIM(RTRIM([fechaOrden])) END AS [fechaOrden]
		,CASE WHEN [fechaCancelacion] IS NULL THEN '1/1/1970 12:00:00' ELSE LTRIM(RTRIM([fechaCancelacion])) END AS [fechaCancelacion]
		,CASE WHEN [fechaEntregaPrometida] IS NULL THEN '1/1/1970 12:00:00' ELSE LTRIM(RTRIM([fechaEntregaPrometida])) END AS [fechaEntregaPrometida]
		,CASE WHEN [fechaEnvioPrometida] IS NULL THEN '1/1/1970 12:00:00' ELSE LTRIM(RTRIM([fechaEnvioPrometida])) END AS [fechaEnvioPrometida]
		,CASE WHEN [fechaFacturaVenta] IS NULL THEN '1/1/1970 12:00:00' ELSE LTRIM(RTRIM([fechaFacturaVenta])) END AS [fechaFacturaVenta]
		,CASE WHEN [fechaEntrega] IS NULL THEN '1/1/1970 12:00:00' ELSE LTRIM(RTRIM([fechaEntrega])) END AS [fechaEntrega]
		,CASE WHEN [fechaActualizacion] IS NULL THEN '1/1/1970 12:00:00' ELSE LTRIM(RTRIM([fechaActualizacion])) END AS [fechaActualizacion]
		,CAST(GETDATE() AS DATE) AS FC_Fecha_Creacion
		FROM STG.[dbo].[STG_VentasEmitidas]
			LEFT JOIN LK_UnidadNegocio U
		ON LTRIM(RTRIM([unidadNegocio])) = U.CD_ID_Origen
		LEFT JOIN LK_Articulo A
		ON [numeroArticulo] = A.CD_ID_Origen
		LEFT JOIN LK_Tipo_Documento D
		ON LTRIM(RTRIM([tipoOrden])) = D.CD_ID_Origen
		LEFT JOIN LK_Cliente C
		ON [numeroCliente] = C.CD_ID_Origen) AS tablaAgrupada 
												WHERE agrupado = 1
		) AS STG ([agrupado],[ID_VentasEmitidas],SK_UNI_UnidadNegocio,SK_ART_Articulo,[numeroOrdenCompania],[numeroOrdenVenta],[numeroLinea],[numeroSufijoOrden],SK_TDO_Tipo_Documento,[numeroSerie],[numeroFactura],SK_CLI_Cliente,[numeroOrdenVentaOriginal],SK_TDO_Tipo_Documento_Original,[codigoEstadoUltimo],[codigoEstadoSiguiente],[unidadMedidaVenta],[cantidadEnviada],[costoUnitario],[precioTotalPesos],[fechaRequerido],[fechaOrden],[fechaCancelacion],[fechaEntregaPrometida],[fechaEnvioPrometida],[fechaFacturaVenta],[fechaEntrega],[fechaActualizacion],FC_Fecha_Creacion)
ON VEN.[NU_Orden_Venta] = STG.[numeroOrdenVenta]
	AND VEN.[NU_NumeroLinea] = STG.[numeroLinea]
	AND VEN.[SK_TDO_Tipo_Documento] = STG.[SK_TDO_Tipo_Documento]
	AND VEN.[NU_Orden_Compania] = STG.[numeroOrdenCompania]
WHEN MATCHED THEN
      UPDATE SET
		VEN.[SK_UNI_UnidadNegocio]= STG.[SK_UNI_UnidadNegocio]
		,VEN.[SK_ART_Articulo] = STG.SK_ART_Articulo
		,VEN.[NU_Orden_Compania] = STG.[numeroOrdenCompania]
		,VEN.[NU_Orden_Venta] = STG.[numeroOrdenVenta]
		,VEN.[NU_NumeroLinea] = STG.[numeroLinea]
		,VEN.[VC_NumeroSufijoOrden] = STG.[numeroSufijoOrden]
		,VEN.[SK_TDO_Tipo_Documento] = STG.[SK_TDO_Tipo_Documento]
		,VEN.[VC_Numero_Serie] = STG.[numeroSerie]
		,VEN.[VC_Numero_Factura] = STG.[numeroFactura]
		,VEN.[SK_CLI_Cliente] = STG.SK_CLI_Cliente
		,VEN.[NU_Orden_Venta_Original] = STG.[numeroOrdenVentaOriginal]
		,VEN.SK_TDO_Tipo_Documento_Original = STG.SK_TDO_Tipo_Documento_Original
		,VEN.[VC_Codigo_Estado_Ultimo] = STG.[codigoEstadoUltimo]
		,VEN.[VC_Codigo_Estado_Siguiente] = STG.[codigoEstadoSiguiente]
		,VEN.[VC_Unidad_Medida_Venta] = STG.[unidadMedidaVenta]
		,VEN.[NU_Cantidad] = STG.[cantidadEnviada]
		,VEN.[NU_CostoUnitario] = STG.[costoUnitario]
		,VEN.[NU_PrecioTotalPesos] = STG.[precioTotalPesos]
		,VEN.[FC_Fecha_Requerido] = STG.[fechaRequerido]
		,VEN.[FC_Fecha_Orden] = STG.[fechaOrden]
		,VEN.[FC_Fecha_Cancelacion] = STG.[fechaCancelacion]
