                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select Distinct SK_OBJ_ObjetivoComprador, 
getdate() FC_OBJ_Ejecucion ,
fecha_inicio_mes FC_OBJ_FechaEvaluacion,
 nu_OBJ_ValorObjetivo,
  '*Sin Dato*'SD            
  ,'*Sin Dato*' SND,
  fecha_inicio_mes FC_OBJ_Fecha,
FC_OBJ_InicioMedicion, 
FC_OBJ_FinMedición  , gg.Monto, gg.tipo 

,fecha_fin_mes  from (
select  'Entregas Lineas Vencidas General' as EntregasLineasVencidas, cast ( ? as date)  fecha_inicio_mes ,  cast ( ? as date)  fecha_fin_mes , 'Parcial' as tipo , 
isnull (monto,0) as Monto
from
(
select  cast (? as date) inicio, cast (? as date)  fin,  CASE WHEN
LineasPendientes = 0 THEN 0 
ELSE ROUND( lineaspend7 * 100.00 / LineasPendientes, 2)/100  
end monto
FROM 
(
select cast (? as date) inicio, cast (? as date)  fin, count (*)  lineaspend7 from 
(
select 
 distinct  
TipoDeOrden,
Compania,
NroOrdenCompra,
SufijoOrdenCompra,
NroDeLinea
from [dbo].[STG_OrdenCompraLineas] F4311
inner join [dbo].[STG_Recepcion] F43121 on
F43121. [TipoOrden] 		= F4311.TipoDeOrden AND 
F43121.[NroCompaniaOrden]		= F4311.Compania AND 
F43121.[NroOrden]		= F4311.NroOrdenCompra AND 
F43121.[NroSufijo]	 	= F4311.SufijoOrdenCompra AND 
F43121.[NroLinea]   	= F4311.NroDeLinea AND	
F43121.[TipoDocumento] = 'OV'			and 
F43121.TipoRegistroCoincidencia = 1 
inner join 
(select VC_APROB_Compania, NU_APROB_NUMERO, VC_APROB_Tipo, VC_APROB_Sufixo, MAX (FC_APROB_FechaHoraMov ) FechaAprob
from [dbo].STG_BT_AprobacionDocumento  
WHERE vc_aprob_CODIGO = '3N'
GROUP BY VC_APROB_Compania, NU_APROB_NUMERO, VC_APROB_Tipo, VC_APROB_Sufixo) APROBO1
ON 
VC_APROB_Compania		=   Compania  AND 
 NU_APROB_NUMERO	=    NroOrdenCompra  AND
 VC_APROB_Tipo			=	TipoDeOrden  
where F4311.TipoDeOrden IN ('OP','OD','O5','LP') 
and CodigoDeEstadoSiguiente = 999 
and CodigoDeEstadoultimo != 980 
and  FechaAprob &lt; cast (? as date) 
and   dateadd(day, 7,FechaEntregaPrometidaOriginal )   between  cast (? as date)  and cast (? as date) 
and   dateadd(day, 7,[FechaPrometidaOriginal] ) &lt; [FechaRecepcion]
union
select 
 distinct  
TipoDeOrden,
Compania,
NroOrdenCompra,
SufijoOrdenCompra,
NroDeLinea
from [dbo].[STG_OrdenCompraLineas] F4311
 inner join 
(select VC_APROB_Compania, NU_APROB_NUMERO, VC_APROB_Tipo, VC_APROB_Sufixo, MAX (FC_APROB_FechaHoraMov ) FechaAprob
from [dbo].STG_BT_AprobacionDocumento  
WHERE vc_aprob_CODIGO = '3N'
GROUP BY VC_APROB_Compania, NU_APROB_NUMERO, VC_APROB_Tipo, VC_APROB_Sufixo) APROBO1
ON VC_APROB_Compania		=   Compania  AND 
 NU_APROB_NUMERO	=    NroOrdenCompra  AND
 VC_APROB_Tipo			=	TipoDeOrden  
where F4311.TipoDeOrden IN ('OP','OD','O5','LP') 
and  FechaAprob &lt; cast (? as date) 
and CodigoDeEstadoultimo &gt; 230 and  CodigoDeEstadoultimo &lt; 980 and  CodigoDeEstadoSiguiente != 999 and  TipoDeLinea &lt;&gt; 'AC'
and  dateadd(day, 7,FechaEntregaPrometidaOriginal )  between  cast (? as date)  and cast (? as date) 
) lineas_vto7d_historicas 
) vto7 
inner join
(
select  cast (? as date)   fin, count (*) LineasPendientes 
from 
(
select 
 distinct  
TipoDeOrden,
Compania,
NroOrdenCompra,
SufijoOrdenCompra,
NroDeLinea
from [dbo].[STG_OrdenCompraLineas] F4311
inner join [dbo].[STG_Recepcion] F43121 on
F43121. [TipoOrden] 		= F4311.TipoDeOrden AND 
F43121.[NroCompaniaOrden]		= F4311.Compania AND 
