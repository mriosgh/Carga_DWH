     ,cast('Tarea' as nvarchar(8)) as Tipo
     ,[OwnerId]
     ,[SK_USU_OwnerId]
     ,[Subject]
     ,[Description]
     ,[CreatedById]
     ,[SK_USU_CreatedById]
     ,[CallDurationInSeconds]
