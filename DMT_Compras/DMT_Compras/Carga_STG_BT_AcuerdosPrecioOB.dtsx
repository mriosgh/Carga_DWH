              refId="Package\Data Flow Task\Source Aprobacion"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Source Aprobacion"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/**Recordar identificar  previamente documentos de compras y OR cON todas sus lineas en estado canceladas PDLTTR  = 980 segun documento diseño para asignar estado **/
	SELECT 
  		 CASE
			WHEN  ltrim(ULT_APROB.Estado_aprobación) like '%R' THEN 'Rechazada'
			WHEN  ltrim(ANTE_ULT_APROB.Estado_aprobación) like '3N' AND ltrim(ULT_APROB.Estado_aprobación) like '3N' THEN 'Asignado al Comprador'
			WHEN  ltrim(ANTE_ULT_APROB.Estado_aprobación) like '3N' AND ltrim(ULT_APROB.Estado_aprobación) like 'O'  THEN 'Asignado al Comprador'
			WHEN  ltrim(ULT_APROB.Estado_aprobación) like '3N' AND ltrim(ANTE_ULT_APROB.Estado_aprobación )like '%R' THEN 'Rechazada'
			WHEN  ltrim(ULT_APROB.Estado_aprobación) like '3N' AND ltrim(ANTE_ULT_APROB.Estado_aprobación) like '%A' THEN 'Aprobada'
			WHEN  ltrim(ULT_APROB.Estado_aprobación) = '2A' AND ltrim(ANTE_ULT_APROB.Estado_aprobación) ='3N' THEN 'Aprobada'
			WHEN  ltrim(ULT_APROB.Estado_aprobación) like '3N' AND ltrim (ANTE_ULT_APROB.Estado_aprobación) like 'O' THEN 'Asignado al Comprador'
			WHEN  ltrim(ULT_APROB.Estado_aprobación) like '2N' THEN	 'Pendiente Firma'
			WHEN  ltrim(ULT_APROB.Estado_aprobación) like '%B'  THEN 'Pendiente Firma'
			WHEN ltrim( ULT_APROB.Estado_aprobación) like 'O'  THEN	 'Pendiente Firma'
			ELSE 'Aprobacion No Completada'
		 END Estado_Aprob
		,CASE
			WHEN  ltrim(ULT_APROB.Estado_aprobación) like '2N' then  Responsable
			ELSE ''
		 END PROX_FIRMA
		,ULT_APROB.Unidad_de_negocio 
		,ULT_APROB.Documento_nro_orden
		,ULT_APROB.Documento_Empresa
		,ULT_APROB.Tipo_de_orden
		,ULT_APROB.Sufijo_de_orden
		,ULT_APROB.Estado_aprobación Aprob_Estado
		,ANTE_ULT_APROB.Estado_aprobación Aprob_Estado_anterior
		,ULT_APROB.fecha
		,ULT_APROB.HORA_EVENTO
		,Responsable
	FROM 
	(SELECT * FROM 
	  (SELECT
		ROW_NUMBER() OVER(PARTITION BY  HOKCOO,HODOCO, HODCTO,HOSFXO  ORDER BY CAST (DATEADD (d, CAST(HOTRDJ AS INT) - ((CAST(HOTRDJ AS INT) / 1000) * 1000)
		,CONVERT(DATETIME, STR((CAST(HOTRDJ AS INT) / 1000) + 1900 ) + '0101')) - 1  AS DATE)DESC  ,HORDT DESC, HOASTS DESC ) AS ULT
		,HOMCU Unidad_de_negocio --oc
		,HOKCOO Documento_Empresa --oc
		,HODOCO Documento_nro_orden --oc
		,HODCTO	Tipo_de_orden --oc
		,HOSFXO	Sufijo_de_orden --oc
		,CAST (DATEADD(d, CAST(HOTRDJ AS INT) - ((CAST(HOTRDJ AS INT) / 1000) * 1000)
		,CONVERT(DATETIME, STR((CAST(HOTRDJ AS INT) / 1000) + 1900 ) + '0101')) - 1  AS DATE) FECHA
		,HORDT HORA_EVENTO
		,HOASTS	Estado_aprobación
		,PERSONA.ABALPH Responsable		
	 FROM [PRODDTA].[F4209] APROBOC
		LEFT JOIN [PRODDTA].F0101 PERSONA 
		ON PERSONA.ABAN8 = APROBOC.HORPER)ULT		
	 WHERE ULT = 1) ULT_APROB
			LEFT JOIN (SELECT * FROM 
	(SELECT
		ROW_NUMBER() OVER(PARTITION BY  HOKCOO,HODOCO, HODCTO,HOSFXO  ORDER BY CAST (DATEADD (d, CAST(HOTRDJ AS INT) - ((CAST(HOTRDJ AS INT) / 1000) * 1000)
		,CONVERT(DATETIME, STR((CAST(HOTRDJ AS INT) / 1000) + 1900 ) + '0101')) - 1  AS DATE)DESC  ,HORDT DESC, HOASTS DESC ) AS ULT
		,HOMCU Unidad_de_negocio --oc
		,HOKCOO Documento_Empresa--oc
		,HODOCO Documento_nro_orden --oc
		,HODCTO Tipo_de_orden--oc
		,HOSFXO Sufijo_de_orden	--oc
		,CAST (DATEADD (d, CAST(HOTRDJ AS INT) - ((CAST(HOTRDJ AS INT) / 1000) * 1000)
		,CONVERT(DATETIME, STR((CAST(HOTRDJ AS INT) / 1000) + 1900 ) + '0101')) - 1  AS DATE) FECHA
		,HORDT HORA_EVENTO
		,HOASTS	Estado_aprobación			
	  FROM [PRODDTA].[F4209] aproboc
		LEFT JOIN [PRODDTA].F0101 PERSONA 
		ON PERSONA.ABAN8 =aproboc.HORPER) ULT_APROB		
	  WHERE ult = 2)  ANTE_ULT_APROB ON ANTE_ULT_APROB.Documento_Empresa = ULT_APROB.Documento_Empresa 	   
	  AND ANTE_ULT_APROB.Documento_nro_orden = ULT_APROB.Documento_nro_orden	  
	  AND ANTE_ULT_APROB.Tipo_de_orden = ULT_APROB.Tipo_de_orden		
	  AND ANTE_ULT_APROB.Sufijo_de_orden= ULT_APROB.Sufijo_de_orden	
	WHERE ULT_APROB.Tipo_de_orden = 'OB'</property>
