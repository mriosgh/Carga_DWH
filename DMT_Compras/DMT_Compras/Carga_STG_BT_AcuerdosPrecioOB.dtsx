                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT OB.PDMCU, 
OB.PDKCOO,
OB.PDDOCO,
OB.PDDCTO,
OB.PDSFXO,
OB.PDLNID,
OB.PDAN8, 
CAST (  DateAdd (d, cast(OB.PDTRDJ   as int) - ((cast(OB.PDTRDJ as int) / 1000) * 1000),
convert(datetime, str((cast(OB.PDTRDJ as int) / 1000) + 1900 ) + '0101')) -1  AS DATE) as PDTRDJ ,
CAST (  DateAdd (d, cast(OB.PDCNDJ   as int) - ((cast(OB.PDCNDJ as int) / 1000) * 1000),
convert(datetime, str((cast(OB.PDCNDJ as int) / 1000) + 1900 ) + '0101')) -1  AS DATE) as PDCNDJ,
