OB.PDDSC2,
OB.PDLNTY,
OB.PDNXTR,
OB.PDLTTR,
OB.PDUOM,
OB.PDUORG,
OB.PDCRCD,
OB.PDPRRC,
OB.PDFRRC,
SUM(O5.PDUORG) NU_OBLP_CantidadesAdquiridas,
COUNT (DISTINCT (O5.PDLNID)) NU_OBLP_LineasAdquiridas,
SUM(O5.PDPRRC) NU_OBLP_ImporteAdquirido
FROM PRODDTA.F4311 OB
LEFT JOIN PRODDTA.F4311 O5 ON O5.PDCO = OB.PDKCOO AND O5.PDOKCO = OB.PDKCOO AND O5.PDOORN = OB.PDDOCO 
AND O5.PDOCTO = OB.PDDCTO AND O5.PDOGNO = OB.PDLNID AND O5.PDDCTO ='O5' AND O5.PDLTTR &lt;&gt; '980'
WHERE OB.PDDCTO ='OB'
