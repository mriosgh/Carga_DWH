                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT T1.A6AN8 PRO_Proveedor
,T2.DCAC08 CodCriticidadCategoria
,A3.DRDL01 CriticidadCategoria
,T2.DCAC06 Cod_CriticidadCalidad
,A4.DRDL01 CriticidadCalidad
,T2.DCAC04 CodCriticidadEvalTec
,A5.DRDL01 CriticidadEvalTec
,T1.A6HDPY CodRetencionPago
,UPPER (Concat (ltrim (rtrim (A6.DRDL01)) ,'. ', ltrim (rtrim (A6.DRDL02 )))) RetencionPago
,T3.AOEE03 EstadoAon
,A1.DRDL01 Desc_EstadoAon
,T3.AOEE02 TipoProveedor
,A2.DRDL01 Desc_TipoProveedor
,CAST ( DateAdd (d, cast(T3.AOFCAON   as int) - ((cast(T3.AOFCAON as int) / 1000) * 1000),
 convert(datetime, str((cast(T3.AOFCAON as int) / 1000) + 1900 ) + '0101')) -1  AS DATE) AS FechaBloqueo
FROM PRODDTA.F0401 T1
LEFT JOIN PRODDTA.F550401D T2
ON T2.DCAN8 = T1.A6AN8
LEFT JOIN PRODDTA.F55AON T3
ON T1.A6AN8 = T3.AOAN8
LEFT JOIN PRODCTL.F0005 A3
ON LTRIM (RTRIM(T2.DCAC08)) = LTRIM (RTRIM(A3.DRKY))
AND LTRIM (RTRIM(A3.DRSY)) ='55' AND LTRIM (RTRIM(A3.DRRT)) ='08' --Criticidad Categoria
LEFT JOIN PRODCTL.F0005 A4
ON LTRIM (RTRIM(T3.AOTUAON)) = LTRIM (RTRIM(A4.DRKY))
AND LTRIM (RTRIM(A4.DRSY)) ='55'AND LTRIM (RTRIM(A4.DRRT)) ='06'--Criticidad Calificacion
LEFT JOIN PRODCTL.F0005 A5
ON LTRIM (RTRIM(T2.DCAC04)) = LTRIM (RTRIM(A5.DRKY))
AND LTRIM (RTRIM(A5.DRSY)) ='55'AND LTRIM (RTRIM(A5.DRRT)) ='04' --Criticidad Evaluacion
LEFT JOIN PRODCTL.F0005 A1
ON LTRIM (RTRIM(T3.AOEE03)) = LTRIM (RTRIM(A1.DRKY))
AND LTRIM (RTRIM(A1.DRSY)) ='55' AND LTRIM (RTRIM(A1.DRRT)) ='A4' --AON Estado Proveedor
LEFT JOIN PRODCTL.F0005 A2
ON LTRIM (RTRIM(T3.AOEE02)) = LTRIM (RTRIM(A2.DRKY))
AND LTRIM (RTRIM(A2.DRSY)) ='55'   AND LTRIM (RTRIM(A2.DRRT)) ='A3' --AON Tipo Proveedor
LEFT JOIN PRODCTL.F0005 A6
ON LTRIM (RTRIM(T1.A6HDPY)) = LTRIM (RTRIM(A6.DRKY))
AND LTRIM (RTRIM(A6.DRSY)) = 'H01'AND LTRIM (RTRIM(A6.DRRT)) ='HD'--Pago Retenido</property>
