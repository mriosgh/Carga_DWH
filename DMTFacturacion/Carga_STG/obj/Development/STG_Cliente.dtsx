                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Select 'IDJDE'                       = F101.ABAN8,
       'RazonSocial'                 = LTRIM(F101.ABALPH),
	   'CUIT'                        = LTRIM(F101.ABTAX),
	   'Tipo'                        = LTRIM(F101.ABAT1),
	   'DescripcionTipo'             = LTRIM((Select DRDL01 FROM [PRODCTL].[F0005] 
											WHERE DRSY = '01'
											  AND DRRT = 'ST'
											  AND LTRIM(DRKY) = LTRIM(F101.ABAT1))),
	   'IddeSalesforce'              = LTRIM(F101.ABALP1),
	   'Rubro'                       = LTRIM(F101.ABSIC),
	   'DescripcionRubro'            = LTRIM((Select DRDL01 FROM [PRODCTL].[F0005] 
											WHERE DRSY = '01'
											  AND DRRT = 'SC'
											  AND LTRIM(DRKY) = LTRIM(F101.ABSIC))),
	   'MensajedeCreditos'           = LTRIM(F101.ABCM),
	   'DescripcionMensajedeCredito' = LTRIM((Select DRDL01 FROM [PRODCTL].[F0005] 
											WHERE DRSY = '00'
											  AND DRRT = 'CM'
											  AND LTRIM(DRKY) = LTRIM(F101.ABCM))),
	   'OtrocodigodeImpuesto'        = LTRIM(F101.ABTX2),
	   'Direccion1'                  = LTRIM(F116.ALADD1),
	   'Direccion2'                  = LTRIM(F116.ALADD2),
	   'Direccion3'                  = LTRIM(F116.ALADD3),
	   'Direccion4'                  = LTRIM(F116.ALADD4),
	   'CodigoPostal'                = LTRIM(F116.ALADDZ),
	   'Ciudad'                      = LTRIM(F116.ALCTY1),
	   'Pais'                        = LTRIM(F116.ALCTR),
	   'DescripcionPais'             = LTRIM((Select DRDL01 FROM [PRODCTL].[F0005] 
											WHERE DRSY = '00'
											  AND DRRT = 'CN'
											  AND LTRIM(DRKY) = LTRIM(F116.ALCTR))),
	   'CodigoProvincia'             = LTRIM(F116.ALADDS),
	   'DescripcionProvincia'        = LTRIM((Select DRDL01 FROM [PRODCTL].[F0005] 
											WHERE DRSY = '00'
											  AND DRRT = 'S'
											  AND LTRIM(DRKY) = LTRIM(F116.ALADDS))),
	   'FechaAltadeCliente'          = DATEADD(dd,((F301.A5DAOJ % 1000) - 1),(cast(cast(1900+(F301.A5DAOJ/1000) as decimal(4,0)) as varchar(4)) + '01' + '01')),
	   'CodigoMonedaCliente'         = LTRIM(F301.A5CRCD),
	   'DescripcionMoneda'           = LTRIM(F013.CVDL01),
	   'CondiciondePago'             = LTRIM(F301.A5TRAR),
	   'DescripcionCondiciondePago'  = LTRIM(F014.PNPTD)
from [JDE_REP].[PRODDTA].[F0101] F101
	 Left Join  [JDE_REP].[PRODDTA].[F0116] F116 
		On F101.ABAN8 = F116.ALAN8
	 Left Join  [JDE_REP].[PRODDTA].[F0301] F301
		On F101.ABAN8 = F301.A5AN8
	left join [JDE_REP].[PRODDTA].[F0013] F013
		On f301.A5CRCD = F013.CVCRCD
	left join [JDE_REP].[PRODDTA].[F0014] F014
		On f301.A5CRCD = F014.PNPTC
WHERE LTRIM(F101.ABAT1) = 'C'</property>
