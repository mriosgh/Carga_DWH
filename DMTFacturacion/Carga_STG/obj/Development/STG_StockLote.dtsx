                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT SUM(LIPQOH)/1000 AS  CANT
	 ,LTRIM(RTRIM(LILOTN)) AS NroSerie
	 ,CAST(LTRIM(RTRIM(LIITM)) AS BIGINT) AS numeroArticulo 
