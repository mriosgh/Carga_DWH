	 ,LTRIM(RTRIM(LIGLPT)) AS categoria
	 ,DATEADD(dd,((LTRIM(RTRIM(LILRCJ)) % 1000) - 1),(cast(cast(1900+(LTRIM(RTRIM(LILRCJ))/1000) as decimal(4,0)) as varchar(4)) + '01' + '01'))  as fechaUltimoMovimiento
