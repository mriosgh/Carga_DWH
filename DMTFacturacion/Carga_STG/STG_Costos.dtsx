                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT	LTRIM(RTRIM(C.COMCU)) AS unidadNegocio
		,LTRIM(RTRIM(A.IMITM)) AS numeroArticulo
		,LTRIM(RTRIM(A.IMLITM)) AS segundoNumeroArticulo
		,DATEADD(dd,((LTRIM(RTRIM(C.COUPMJ)) % 1000) - 1),(cast(cast(1900+(LTRIM(RTRIM(C.COUPMJ))/1000) as decimal(4,0)) as varchar(4)) + '01' + '01')) as fechaActualizacion
		,LTRIM(RTRIM(C.COLEDG)) AS tipoCosto
		,C.COUNCS AS costo
FROM [PRODDTA].[F4101] A
LEFT JOIN [PRODDTA].[F4105] C
ON A.IMITM = C.COITM
WHERE (LTRIM(RTRIM(C.COMCU)) LIKE '901%'
OR LTRIM(RTRIM(C.COMCU)) LIKE '201%')
AND LTRIM(RTRIM(C.COLEDG)) = '07'
ORDER BY LTRIM(RTRIM(A.IMITM));</property>
