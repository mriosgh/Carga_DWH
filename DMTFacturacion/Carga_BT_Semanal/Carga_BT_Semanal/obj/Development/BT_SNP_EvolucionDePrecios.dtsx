          <DTS:Variables />
          <DTS:ObjectData>
            <pipeline
              version="1">
              <components>
                <component
                  refId="Package\Foreach Loop Container\TMP_PreciosTrimestre\PreciosPromedioPonderado"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="PreciosPromedioPonderado"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">WITH PreciosHistoricos AS (
	SELECT
		'Historicos' AS BT_ORIGEN,
		A.VC_ART_SndItemNumber,
		M.SK_Trimestre,
		ROW_NUMBER() OVER(PARTITION BY A.VC_ART_SndItemNumber ORDER BY M.SK_Trimestre DESC) NroROW,
		--AVG(L.NU_LCH_ValorUnitarioExtranjero) AS PrecioPromedio
		IIF(SUM(NU_LCH_UnidadesSolicitadas) = 0 , 0, SUM(L.NU_LCH_ValorTotalExtranjero) / SUM(L.NU_LCH_UnidadesSolicitadas))  PrecioPromedio		
	FROM BT_LineasCompraHistorica L
	INNER JOIN LK_Articulo A
		ON A.SK_ART_Articulo = L.SK_ART_Articulo
		AND L.SK_ART_Articulo != -1
		AND L.FC_LCH_FechaLnea &gt;= '2016-01-01'
		AND VC_LCH_TipoLnea != 'AC'
	INNER JOIN LK_Fecha F
		ON L.FC_LCH_FechaLnea = F.FC_Fecha
	INNER JOIN LK_Mes M
		ON F.SK_Mes = M.SK_Mes
	INNER JOIN LK_UnidadNegocio UNI
		ON L.SK_UNI_UnidadNegocio = UNI.SK_UNI_UnidadNegocio
	WHERE L.FC_Fecha &lt;= ?
			AND UNI.CD_ID_Origen IN ('201', '202', '302', '458', '458BA', '901', '901BA')
	GROUP BY M.SK_Trimestre, A.VC_ART_SndItemNumber
),


PreciosPendientes AS (
	SELECT
		'Pendientes' AS BT_ORIGEN,
		A.VC_ART_SndItemNumber,
		M.SK_Trimestre,
		ROW_NUMBER() OVER(PARTITION BY A.VC_ART_SndItemNumber ORDER BY M.SK_Trimestre DESC) NroROW,
		--AVG(L.NU_LPE_ValorUnitarioExtranjero) AS PrecioPromedio
		IIF(SUM(L.NU_LPE_UnidadesSolicitadas) = 0, 0, SUM(L.NU_LPE_ValorTotalExtranjero) / SUM(L.NU_LPE_UnidadesSolicitadas)) AS PrecioPromedio
	FROM BT_LineasPendientesEntrega L
	INNER JOIN LK_Articulo A
		ON A.SK_ART_Articulo = L.SK_ART_Articulo
		AND L.SK_ART_Articulo != -1	
		AND MONTH(L.[FC_LPE_FechaLnea ]) in (3,6,9,12)
		AND DAY(L.[FC_LPE_FechaLnea ]) = DAY(EOMONTH( L.[FC_LPE_FechaLnea ]) )
		AND [VC_ORL_TipoLnea ] != 'AC'
	INNER JOIN LK_Fecha F
		ON L.[FC_LPE_FechaLnea ] = F.FC_Fecha
	INNER JOIN LK_Mes M
		ON F.SK_Mes = M.SK_Mes
	INNER JOIN LK_UnidadNegocio UNI
