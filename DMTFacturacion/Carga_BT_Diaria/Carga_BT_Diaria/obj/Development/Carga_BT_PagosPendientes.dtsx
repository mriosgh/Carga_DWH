                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
	CAST(CP.TipoDeDocumento AS NVARCHAR(3)) AS TipoDeDocumento
	,CP.NroDocumento
	,CAST(CP.DocumentoPagarArticulo AS NVARCHAR(5)) AS DocumentoPagarArticulo
	,CAST(LTRIM(RTRIM(CP.NroDeFacturaDelProveedor)) AS NVARCHAR(20)) AS NroDeFacturaDelProveedor
	,CAST(CP.CodigoDeEstadoDePago AS NVARCHAR(2)) AS CodigoDeEstadoDePago

	,R.TipoOrden							--TIPO ORDEN DE COMPRA
	,R.NroOrden								--NRO ORDEN COMPRA
	,ISNULL(CAST(CP.IdProveedor AS int),0) AS IdProveedor
	,LTRIM(RTRIM(R.CuentaObjeto)) AS CuentaObjeto
	,LTRIM(RTRIM(R.UnidadNegocio)) AS UnidadDeNegocio
	,CAST(LTRIM(RTRIM(R.nroArticulo)) AS INT) AS NroArticulo

	,CP.FechaVencimientoNeto
	,CP.FechaFactura
	,R.FechaGL								--FECHA MATCHING

	,CP.ValorBruto
                   ,CP.ValorImponible
	,DATEDIFF(DAY,CP.FechaFactura,GETDATE()) AS NU_DiasFactura
	,DATEDIFF(DAY,CP.FechaVencimientoNeto,GETDATE()) AS NU_DiasVencimiento

FROM  STG_CuentasPagar CP
	INNER JOIN STG_Recepcion R
		ON CP.NroDocumento = R.NroDocumento
		AND CP.TipoDeDocumento = R.TipoDocumento
		AND CP.DocumentoPagarArticulo = R.ItemDocumentoPago
		AND R.TipoRegistroCoincidencia = 2
WHERE
	CP.TipoDeDocumento IN ('PL','XF')
	AND CP.CodigoDeEstadoDePago != 'P'
	AND CP.TipoDeDocumentoOrdenDeCompra in ('OP','LP','O5','OD')
	AND CP.FechaFactura &gt;= '2016-01-01'</property>
