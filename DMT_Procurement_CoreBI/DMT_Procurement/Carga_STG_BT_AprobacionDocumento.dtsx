            <component
              refId="Package\Carga_STG_BT_AprobacionDocumento\OLE DB Source"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="OLE DB Source"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
			ULT ,						--PK1
			UDocumento_Empresa	,		--PK1
			UDocumento_nro_orden	,	--PK1
			UTipo_de_orden	,			--PK1	
			USufijo_de_orden	,		--PK1	
			UUnidad_de_negocio, 
			case when UEstado_aprobación ='3N' then Solicitante else USolicitante end SK_EMP_Empleado_Responsable, 
			UEstado_aprobación,
			case
					WHEN	UEstado_aprobación					like '%R'														THEN 'Rechazada'
					WHEN	ante_ult_Aprob.Estado_aprobación	= '3N'	AND UEstado_aprobación = '3N'							THEN 'Asignado al solicitante'
					WHEN	ante_ult_Aprob.Estado_aprobación	= '3N'	AND UEstado_aprobación = 'O'							THEN 'Asignado al solicitante'
					when	UEstado_aprobación					= '3N'	AND ltrim( ante_ult_Aprob.Estado_aprobación)like '%R'	THEN 'Rechazada'
					WHEN	UEstado_aprobación					= '3N'	AND ante_ult_Aprob.Estado_aprobación  like '%A'			THEN 'Aprobada'
					WHEN	UEstado_aprobación					= '2A'	AND ante_ult_Aprob.Estado_aprobación  ='3N'				THEN 'Aprobada - Aprobación Pendiente'
					WHEN	UEstado_aprobación					= '3N'	AND ltrim (ante_ult_Aprob.Estado_aprobación) = 'O'		THEN 'Asignado al solicitante'
					WHEN	UEstado_aprobación					= '2N'															THEN 'Pendiente Aprobación'
					WHEN	UEstado_aprobación					like '%B'														THEN 'Aprobación En Curso'
					WHEN	ltrim(UEstado_aprobación)			= 'O'															THEN 'Pendiente Aprobación'
					when	UEstado_aprobación					= '2A'															THEN 'Aprobación En Curso'
			 else  'Aprobación No Completada'
			 end VC_APROB_Estado, 
	CONVERT (VARCHAR (24),CONCAT (ULT_APROB.FechaMovimiento ,' ', CONVERT(VARCHAR(15), Stuff(Stuff( 
	case when ULT_APROB.UHORA_EVENTO&lt; 100 then concat ('0000', convert (varchar(6), ULT_APROB.UHORA_EVENTO ) ) 
	else (case when ULT_APROB.UHORA_EVENTO&lt; 1000 then concat ('000', convert (varchar(6), ULT_APROB.UHORA_EVENTO ) )
	else (case when ULT_APROB.UHORA_EVENTO&lt; 10000 then concat ('00', convert (varchar(6), ULT_APROB.UHORA_EVENTO ) ) 
	else (case when ULT_APROB.UHORA_EVENTO&lt; 100000 then concat ('0', convert (varchar(6), ULT_APROB.UHORA_EVENTO ) ) 
	else convert (varchar(6), ULT_APROB.UHORA_EVENTO)end)end )end) end , 3, 0, ':'), 6, 0, ':'), 8) )) FC_APROB_FechaHoraMov , 
	
	ante_ult_Aprob.HORA_EVENTO,
	CONVERT (VARCHAR (24),CONCAT (ante_ult_Aprob.FECHA ,' ', CONVERT(VARCHAR(15), Stuff(Stuff( 
	case when ante_ult_Aprob.HORA_EVENTO&lt; 100 then concat ('0000', convert (varchar(6), ante_ult_Aprob.HORA_EVENTO ) )
	else ( case when ante_ult_Aprob.HORA_EVENTO&lt; 1000 then concat ('000', convert (varchar(6), ante_ult_Aprob.HORA_EVENTO ) ) 
	else ( case when ante_ult_Aprob.HORA_EVENTO&lt; 10000 then concat ('00', convert (varchar(6), ante_ult_Aprob.HORA_EVENTO ) ) 
	else ( case when ante_ult_Aprob.HORA_EVENTO&lt; 100000 then concat ('0', convert (varchar(6), ante_ult_Aprob.HORA_EVENTO ) ) 
	else convert (varchar(6), ante_ult_Aprob.HORA_EVENTO )end)end)end) end , 3, 0, ':'), 6, 0, ':'), 8) ))			 FC_APROB_FechaHoraEnviado , 
	'' NU_APROB_CantidadDias,	--CALCULAR EN SSIS
	'' NU_APROB_CantidadHoras,	--CALCULAR EN SSIS
    ULT_APROB.FechaMovimiento FC_FECHA_FechaMov, 
	CASE 
			WHEn UEstado_aprobación		= '3N'	THEN ORI
			WHEN UEstado_aprobación		= '2N'	THEN  USOLICITANTE
			ELSE  '-1'
	END   SK_EMP_Empleado_ProxResponsable 
 FROM 
	(SELECT
			ROW_NUMBER() OVER(PARTITION BY  LTRIM(RTRIM(HOKCOO)),HODOCO, LTRIM(RTRIM(HODCTO)),LTRIM(RTRIM(HOSFXO))  ORDER BY    CAST (     DateAdd (d, cast(HOTRDJ as int) - ((cast(HOTRDJ as int) / 1000) * 1000), convert(datetime, str((cast(HOTRDJ as int) / 1000) + 1900 ) + '0101')) - 1  AS DATE) asc, HORDT asc, LTRIM(RTRIM(HOASTS)) asc ) AS ULT 	,
			LTRIM(RTRIM(HOMCU))		UUnidad_de_negocio	,	   --oc
			LTRIM(RTRIM(HOKCOO))		UDocumento_Empresa	,--oc
			HODOCO		UDocumento_nro_orden	,
			LTRIM(RTRIM(HODCTO))		UTipo_de_orden	,								--oc
			LTRIM(RTRIM(HOSFXO))		USufijo_de_orden	,									--oc
			CAST (     DateAdd (d, cast(HOTRDJ as int) - ((cast(HOTRDJ as int) / 1000) * 1000), convert(datetime, str((cast(HOTRDJ as int) / 1000) + 1900 ) + '0101')) - 1  AS DATE)	FechaMovimiento		,
			HORDT		UHORA_EVENTO,
			LTRIM(RTRIM(HOASTS	))	UEstado_aprobación,
			HOAN8 		uorigianate,
			HORPER		uSOlicitante		
	 FROM [PRODDTA].[F4209]   
			WHERE LTRIM(RTRIM(hoDCTO)) IN ('OP','LP','OD','OR','O5','OB')
	) ULT_APROB
 left join 
 (	SELECT
			ROW_NUMBER() OVER(PARTITION BY  LTRIM(RTRIM(HOKCOO)),HODOCO, LTRIM(RTRIM(HODCTO)),LTRIM(RTRIM(HOSFXO))   ORDER BY  CAST (     DateAdd (d, cast(HOTRDJ as int) - ((cast(HOTRDJ as int) / 1000) * 1000), convert(datetime, str((cast(HOTRDJ as int) / 1000) + 1900 ) + '0101')) - 1  AS DATE) asc  ,HORDT asc, LTRIM(RTRIM(HOASTS)) asc ) +1 AS ant_ULT 	,
			LTRIM(RTRIM(HOMCU))		Unidad_de_negocio	,	   --oc
			LTRIM(RTRIM(HOKCOO))		Documento_Empresa	,--oc
			HODOCO		Documento_nro_orden	,		--oc
			LTRIM(RTRIM(HODCTO))		Tipo_de_orden	,								--oc
			LTRIM(RTRIM(HOSFXO))		Sufijo_de_orden	,									--oc
			CAST (     DateAdd (d, cast(HOTRDJ as int) - ((cast(HOTRDJ as int) / 1000) * 1000), convert(datetime, str((cast(HOTRDJ as int) / 1000) + 1900 ) + '0101')) - 1  AS DATE)	FECHA		,
			HORDT	 HORA_EVENTO,
			LTRIM(RTRIM(HOASTS	))		Estado_aprobación	,
			HOAN8	ORI,
			HORPER  SOlicitante		
	 FROM [PRODDTA].[F4209] 
			WHERE LTRIM(RTRIM(hoDCTO)) IN ('OP','LP','OD','OR','O5','OB')
	)    ante_ult_Aprob 
								on			ante_ult_Aprob.Documento_Empresa	= ULT_APROB.UDocumento_Empresa 	   
										and ante_ult_Aprob.Documento_nro_orden	= ULT_APROB.UDocumento_nro_orden	  
										and ante_ult_Aprob.Tipo_de_orden		= ULT_APROB.UTipo_de_orden		
										and ante_ult_Aprob.Sufijo_de_orden		= ULT_APROB.USufijo_de_orden
										and ante_ult_Aprob.ant_ULT				= ULT_APROB.ULT 


      WHERE UTipo_de_orden IN ('OP','LP','OD','OR','O5','OB')
	 
	  order by ULT_APROB.FechaMovimiento asc, UHORA_EVENTO asc, UEstado_aprobación asc</property>
