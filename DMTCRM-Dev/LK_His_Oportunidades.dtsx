                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
       HISTORICO.STEP AS stepN,
       HISTORICO.StageName AS StageNameN,
       PREV.StageName AS StageNameFrom,
       PREV.PSTEP,
	   PREV.CreatedDateFormat AS CreatedDateAnterior,
       HISTORICO.*
FROM (
             SELECT
                    ROW_NUMBER() over(partition by HIST.oportunidadId order by HIST.createdDate) AS STEP,
                    *
             FROM (
                           SELECT
                                  ROW_NUMBER() OVER(PARTITION BY OPORTUNIDADID, STAGENAME ORDER BY CREATEDDATE) AS ROWNUM
                                  ,*
                           FROM STG_Historico_Oportunidades
                           
                           ) HIST
             WHERE
                    HIST.ROWNUM = 1) HISTORICO
       LEFT JOIN (
                           SELECT
                                  ROW_NUMBER() over(partition by oportunidadId order by createdDate) AS PSTEP,
                                  *
                           FROM (
                           SELECT
                                  ROW_NUMBER() OVER(PARTITION BY OPORTUNIDADID, STAGENAME, CREATEDDATEFORMAT ORDER BY CREATEDDATE) AS ROWNUM
                                  ,*
                           FROM STG_Historico_Oportunidades
                          
                           ) BLE 
                           WHERE
                                  BLE.ROWNUM = 1) PREV 
                    ON PREV.OportunidadId = HISTORICO.OportunidadId
                    AND PREV.PSTEP = HISTORICO.STEP - 1</property>
