    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{A4749D74-9E99-4AF3-929B-1000707430B9}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;UPDATE A SET&#xA;       A.[SK_USU_CreatedById] = CASE WHEN A.[CreatedById] = B.[CD_ID_Origen] THEN B.[SK_USU_Usuario] ELSE 0 END&#xA;FROM &quot;+@[$Project::P_Prefijo_STG]+&quot;.[dbo].[STG_Historico_Oportunidades] A&#xA;LEFT JOIN &quot;+@[$Project::P_Prefijo_DW]+&quot;.[dbo].[LK_Usuario] B ON A.[CreatedById] = B.[CD_ID_Origen]&#xA;&#xA;;&#xA;&#xA;UPDATE A SET&#xA;      A.[SK_OPO_Oportunidades] = CASE WHEN A.[OportunidadId] = C.[CD_ID_Origen] THEN C.[SK_OPO_Oportunidades] ELSE 0 END&#xA;FROM &quot;+@[$Project::P_Prefijo_STG]+&quot;.[dbo].[STG_Historico_Oportunidades] A&#xA;LEFT JOIN &quot;+@[$Project::P_Prefijo_DW]+&quot;.[dbo].[LK_Oportunidades] C ON A.[OportunidadId] = C.[CD_ID_Origen]&#xA;&#xA;;&#xA;&#xA;UPDATE &quot;+@[$Project::P_Prefijo_STG]+&quot;.[dbo].[STG_Historico_Oportunidades]&#xA;   SET [CreatedDateFormat] = convert(datetime,[CreatedDate])&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="V_STG_LK_Historico_Oportunidades">
      <DTS:VariableValue
        DTS:DataType="8">UPDATE A SET
       A.[SK_USU_CreatedById] = CASE WHEN A.[CreatedById] = B.[CD_ID_Origen] THEN B.[SK_USU_Usuario] ELSE 0 END
FROM [STG].[dbo].[STG_Historico_Oportunidades] A
LEFT JOIN [DWH_Test].[dbo].[LK_Usuario] B ON A.[CreatedById] = B.[CD_ID_Origen]

;

UPDATE A SET
      A.[SK_OPO_Oportunidades] = CASE WHEN A.[OportunidadId] = C.[CD_ID_Origen] THEN C.[SK_OPO_Oportunidades] ELSE 0 END
FROM [STG].[dbo].[STG_Historico_Oportunidades] A
LEFT JOIN [DWH_Test].[dbo].[LK_Oportunidades] C ON A.[OportunidadId] = C.[CD_ID_Origen]

;

UPDATE [STG].[dbo].[STG_Historico_Oportunidades]
   SET [CreatedDateFormat] = convert(datetime,[CreatedDate])</DTS:VariableValue>
    </DTS:Variable>
