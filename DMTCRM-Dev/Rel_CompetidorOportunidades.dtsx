      DTS:Expression="&quot;UPDATE A SET&#xA;       A.[SK_COP_Competidor] = CASE WHEN A.[SK_OPO_Oportunidades] = B.[SK_OPO_OportunidadId] THEN B.[SK_COP_Competidor] ELSE 0 END&#xA;FROM &quot;+@[$Project::P_Prefijo_DW]+&quot;.[dbo].[LK_Oportunidades] A&#xA;LEFT JOIN &quot;+@[$Project::P_Prefijo_DW]+&quot;.[dbo].[LK_Competidor] B ON A.[SK_OPO_Oportunidades] = B.[SK_OPO_OportunidadId]&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="V_SK_Competidor">
      <DTS:VariableValue
        DTS:DataType="8">UPDATE A SET
       A.[SK_COP_Competidor] = CASE WHEN A.[SK_OPO_Oportunidades] = B.[SK_OPO_OportunidadId] THEN B.[SK_COP_Competidor] ELSE 0 END
FROM [DWH].[dbo].[LK_Oportunidades] A
