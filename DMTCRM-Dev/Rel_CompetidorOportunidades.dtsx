    </DTS:Variable>
    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{758074C2-4119-44B7-9150-8F56977A9336}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;UPDATE A SET&#xA;       A.[SK_USU_CreatedById] = CASE WHEN A.[CreatedById] = B.[CD_ID_Origen] THEN B.SK_USU_Usuario ELSE 0 END&#xA;FROM &quot;+@[$Project::P_Prefijo_STG]+&quot;.[dbo].[STG_LK_Competidor_Oportunidades] A&#xA;LEFT JOIN &quot;+@[$Project::P_Prefijo_DW]+&quot;.[dbo].[LK_Usuario] B ON A.[CreatedById] = B.[CD_ID_Origen]&#xA;&#xA;;&#xA;&#xA;UPDATE A SET&#xA;      A.[SK_USU_LastModifiedById] = CASE WHEN A.[LastModifiedById] = B.[CD_ID_Origen] THEN B.SK_USU_Usuario ELSE 0 END&#xA;FROM &quot;+@[$Project::P_Prefijo_STG]+&quot;.[dbo].[STG_LK_Competidor_Oportunidades] A&#xA;LEFT JOIN &quot;+@[$Project::P_Prefijo_DW]+&quot;.[dbo].[LK_Usuario] B ON A.[LastModifiedById] = B.[CD_ID_Origen]&#xA;&#xA;&#xA;;&#xA;&#xA;UPDATE A SET&#xA;       A.[SK_CUE_Cuenta] = CASE WHEN A.[CuentaId] = C.[CD_ID_Origen] THEN C.[SK_CUE_Cuenta] ELSE 0 END&#xA;FROM &quot;+@[$Project::P_Prefijo_STG]+&quot;.[dbo].[STG_LK_Competidor_Oportunidades] A&#xA;LEFT JOIN &quot;+@[$Project::P_Prefijo_DW]+&quot;.[dbo].[LK_Cuenta] C ON A.[CuentaId] = C.[CD_ID_Origen]&#xA;&#xA;;&#xA;&#xA;UPDATE A SET&#xA;      A.[SK_OPO_Oportunidades] = CASE WHEN A.[OportunidadId] = C.[CD_ID_Origen] THEN C.[SK_OPO_Oportunidades] ELSE 0 END&#xA;FROM &quot;+@[$Project::P_Prefijo_STG]+&quot;.[dbo].[STG_LK_Competidor_Oportunidades] A&#xA;LEFT JOIN &quot;+@[$Project::P_Prefijo_DW]+&quot;.[dbo].[LK_Oportunidades] C ON A.[OportunidadId] = C.[CD_ID_Origen]&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="V_STG_LK_Competidor">
      <DTS:VariableValue
        DTS:DataType="8">UPDATE A SET
       A.[SK_USU_CreatedById] = CASE WHEN A.[CreatedById] = B.[CD_ID_Origen] THEN B.SK_USU_Usuario ELSE 0 END
FROM [STG].[dbo].[STG_LK_Competidor_Oportunidades] A
LEFT JOIN [DWH].[dbo].[LK_Usuario] B ON A.[CreatedById] = B.[CD_ID_Origen]

;

UPDATE A SET
      A.[SK_USU_LastModifiedById] = CASE WHEN A.[LastModifiedById] = B.[CD_ID_Origen] THEN B.SK_USU_Usuario ELSE 0 END
FROM [STG].[dbo].[STG_LK_Competidor_Oportunidades] A
LEFT JOIN [DWH].[dbo].[LK_Usuario] B ON A.[LastModifiedById] = B.[CD_ID_Origen]


;

UPDATE A SET
       A.[SK_CUE_Cuenta] = CASE WHEN A.[CuentaId] = C.[CD_ID_Origen] THEN C.[SK_CUE_Cuenta] ELSE 0 END
FROM [STG].[dbo].[STG_LK_Competidor_Oportunidades] A
LEFT JOIN [DWH].[dbo].[LK_Cuenta] C ON A.[CuentaId] = C.[CD_ID_Origen]

;

UPDATE A SET
      A.[SK_OPO_Oportunidades] = CASE WHEN A.[OportunidadId] = C.[CD_ID_Origen] THEN C.[SK_OPO_Oportunidades] ELSE 0 END
FROM [STG].[dbo].[STG_LK_Competidor_Oportunidades] A
LEFT JOIN [DWH].[dbo].[LK_Oportunidades] C ON A.[OportunidadId] = C.[CD_ID_Origen]</DTS:VariableValue>
    </DTS:Variable>
