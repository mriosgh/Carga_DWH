          <components>
            <component
              refId="Package\Carga STG_Cargos\GET JDE"
              componentClassID="Microsoft.Lookup"
              contactInfo="Lookup;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;6"
              description="Joins additional columns to the data flow by looking up values in a table. For example, join to the 'employee id' column the employees table to get 'hire date' and 'employee name'. We recommend this transformation when the lookup table can fit into memory."
              name="GET JDE"
              usesDispositions="true"
              version="6">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement that generates the lookup table."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	F.IDFacturacion
	,CAST(LTRIM(RTRIM(F.Interno)) AS nvarchar(255)) AS JDE_Interno
	,N.IDActividad AS JDE_idActividad
	,N.IDSegmento AS JDE_idSegmento
	,F0111.WWAN8 AS JDE_idComercial
	,R.IDDeposito AS JDE_idDeposito
	,CAST(ISNULL(CASE
		WHEN cast(F.InicioFac as date) = cast(N.InicioFC as date) THEN 'True'
		ELSE 'False'
	END,'False') AS nvarchar(50)) AS FG_Facturacion_Genuina
	,F.InicioFac
FROM [dbo].[Facturacion] F
LEFT JOIN  [dbo].[Noes] N
	ON F.IDNoe = N.IDNoe
LEFT JOIN  [dbo].[Usuarios] U
	ON U.IDComercial = N.IDComercial 
	AND U.IDComercial &lt;&gt; 0
LEFT JOIN [PRODDTA].[F0111] F0111
	ON LTRIM(RTRIM(F0111.WWREM1)) = LTRIM(RTRIM(U.Email))
	AND F0111.WWIDLN = 0
LEFT JOIN [dbo].[Remitos] R
	ON R.IDRto = F.IDRto</property>
                <property
                  dataType="System.String"
                  description="Specifies a SQL statement that uses parameters to generate the lookup table."
                  expressionType="Notify"
                  name="SqlCommandParam"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select * from (SELECT
	F.IDFacturacion
	,CAST(LTRIM(RTRIM(F.Interno)) AS nvarchar(255)) AS JDE_Interno
	,N.IDActividad AS JDE_idActividad
	,N.IDSegmento AS JDE_idSegmento
	,F0111.WWAN8 AS JDE_idComercial
	,R.IDDeposito AS JDE_idDeposito
	,CAST(ISNULL(CASE
		WHEN cast(F.InicioFac as date) = cast(N.InicioFC as date) THEN 'True'
		ELSE 'False'
	END,'False') AS nvarchar(50)) AS FG_Facturacion_Genuina
	,F.InicioFac
FROM [dbo].[Facturacion] F
LEFT JOIN  [dbo].[Noes] N
	ON F.IDNoe = N.IDNoe
LEFT JOIN  [dbo].[Usuarios] U
	ON U.IDComercial = N.IDComercial 
	AND U.IDComercial &lt;&gt; 0
LEFT JOIN [PRODDTA].[F0111] F0111
	ON LTRIM(RTRIM(F0111.WWREM1)) = LTRIM(RTRIM(U.Email))
	AND F0111.WWIDLN = 0
LEFT JOIN [dbo].[Remitos] R
	ON R.IDRto = F.IDRto) [refTable]
where [refTable].[IDFacturacion] = ?</property>
