'OriginadorDeTransaccion'			= cast(RTRIM(F42119.SDTORG) as nvarchar(10)),
'Enviar_a'						= cast(F42119.SDSHAN as int),
'NotasInternas'					= cast(RTRIM(F42119.SDVR01) as nvarchar(25)),
'CodigoDeRuta'					= cast(RTRIM(F42119.SDROUT) as nvarchar(3)), 
'FechaSolicitada'			= cast(DATEADD(dd,((F42119.SDDRQJ % 1000) - 1),(cast(cast(1900+(F42119.SDDRQJ/1000) as decimal(4,0)) as varchar(4)) + '01' + '01')) as date)

FROM [PRODDTA].[F42119] F42119

LEFT JOIN [PRODDTA].[F0101] F101 
	ON F101.ABAN8 = F42119.SDAN8
LEFT JOIN [PRODDTA].[F0006] F0006
	ON F0006.MCMCU = F42119.SDMCU
LEFT JOIN [PRODCTL].[F0005] F0005
	ON RTRIM(F0005.DRKY) = F42119.SDSRP3
	AND F0005.DRSY = '41'
	AND F0005.DRRT = 'S3'
LEFT JOIN PRODDTA.F4102 v300
	ON cast(RTRIM(LTRIM(F42119.SDITM)) as nvarchar(50)) = v300.ibitm
		and ltrim(rtrim(v300.IBMCU)) = '300'

WHERE 
 cast(RTRIM(LTRIM(F42119.SDDCTO)) as nvarchar(5)) in ('SH', 'SO', 'YS', 'W5', 'W6')
and cast(RTRIM(LTRIM(F42119.SDMCU)) as nvarchar(50)) in ('300','301')
AND cast(RTRIM(F42119.SDNXTR) as INT) &lt; 577
AND CAST(DATEADD(dd,((F42119.SDTRDJ % 1000) - 1),(cast(cast(1900+(F42119.SDTRDJ/1000) as decimal(4,0)) as varchar(4)) + '01' + '01')) as date) &gt;= '2017-01-01'</property>
