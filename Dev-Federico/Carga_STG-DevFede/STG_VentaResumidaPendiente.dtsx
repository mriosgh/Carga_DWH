'NotasInternas'					= cast(RTRIM(F4211.SDVR01) as nvarchar(25)),
'CodigoDeRuta'					= cast(RTRIM(F4211.SDROUT) as nvarchar(3)), 
'FechaSolicitada'			= cast(DATEADD(dd,((F4211.SDDRQJ % 1000) - 1),(cast(cast(1900+(F4211.SDDRQJ/1000) as decimal(4,0)) as varchar(4)) + '01' + '01')) as date)

FROM [PRODDTA].[F4211] F4211

LEFT JOIN [PRODDTA].[F0101] F101 
	ON F101.ABAN8 = F4211.SDAN8
LEFT JOIN [PRODDTA].[F0006] F0006
	ON F0006.MCMCU = F4211.SDMCU
LEFT JOIN [PRODCTL].[F0005] F0005
	ON RTRIM(F0005.DRKY) = F4211.SDSRP3
	AND F0005.DRSY = '41'
	AND F0005.DRRT = 'S3'
LEFT JOIN PRODDTA.F4102 v300
	ON cast(RTRIM(LTRIM(F4211.SDITM)) as nvarchar(50)) = v300.ibitm
		and ltrim(rtrim(v300.IBMCU)) = '300'

WHERE
 cast(RTRIM(LTRIM(F4211.SDDCTO)) as nvarchar(5)) in ('SH', 'SO', 'YS','W5', 'W6')
and cast(RTRIM(LTRIM(F4211.SDMCU)) as nvarchar(50)) in ('300','301')
AND cast(RTRIM(F4211.SDNXTR) AS INT) &lt; 577
AND CAST(DATEADD(dd,((F4211.SDTRDJ % 1000) - 1),(cast(cast(1900+(F4211.SDTRDJ/1000) as decimal(4,0)) as varchar(4)) + '01' + '01')) as date) &gt;= '2017-01-01'</property>
