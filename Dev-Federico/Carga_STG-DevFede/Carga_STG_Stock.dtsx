              description="OLE DB Source"
              name="INPUT Stock"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT	
		 CAST(LTRIM(RTRIM(Stock.LIITM)) AS BIGINT) AS LIITM 
		,LTRIM(RTRIM(Stock.LIMCU)) AS LIMCU   
		,SUM(Stock.LIPQOH)/1000 AS LIPQOH 
		,ISNULL(MAX(Costo.COUNCS),0)/10000 AS CostoUnidad
		,(SUM(Stock.LIPQOH)/1000) * (ISNULL(MAX(Costo.COUNCS),0)/10000) as Monto
		,ISNULL(MAX(Costo02.COUNCS),0)/10000 AS CostoUnidad02 
		,(SUM(Stock.LIPQOH)/1000) * (ISNULL(MAX(Costo02.COUNCS),0)/10000) as Monto02
		,LTRIM(RTRIM(V.IBSRP0))  as IBSRP0  
		,LTRIM(RTRIM(V.IBPRP4))  as IBPRP4
		,LTRIM(RTRIM(V.IBPRP9))  as IBPRP9
		,LTRIM(RTRIM(V.IBPRP0))  as IBPRP0
		,LTRIM(RTRIM(V.IBPRP2))  as IBPRP2
		,LTRIM(RTRIM(v300.IBPRP4))  as IBPRP4_300
		,LTRIM(RTRIM(v300.IBPRP0))  as IBPRP0_300

		FROM [PRODDTA].[F41021] Stock 

		LEFT join PRODDTA.F4102 V
		on Stock.liitm = V.ibitm
		and ltrim(rtrim(V.IBMCU)) = LTRIM(RTRIM(Stock.LIMCU)) COLLATE Modern_Spanish_CI_AS 

		LEFT join PRODDTA.F4102 v300
		on Stock.liitm = v300.ibitm
		and ltrim(rtrim(v300.IBMCU)) = '300'

		LEFT join  [PRODDTA].[F4105] Costo
		on LTRIM(RTRIM(Costo.COMCU)) =  LTRIM(RTRIM(Stock.LIMCU)) COLLATE Modern_Spanish_CI_AS 
		AND Costo.COITM = Stock.LIITM
		AND Costo.COLEDG='35'

		LEFT join  [PRODDTA].[F4105] Costo02
		on LTRIM(RTRIM(Costo02.COMCU)) =  LTRIM(RTRIM(Stock.LIMCU)) COLLATE Modern_Spanish_CI_AS 
		AND Costo02.COITM = Stock.LIITM
		AND Costo02.COLEDG='02'

		
		WHERE 	LIPQOH != 0	
		group by 
			LIITM
			,LIMCU
			,V.IBPRP4
			,V.IBSRP0
			,V.IBPRP9
			,V.IBPRP0
			,V.IBPRP2
			,v300.IBPRP4
			,v300.IBPRP0</property>
