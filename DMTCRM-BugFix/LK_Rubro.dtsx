      DTS:ObjectName="V_STG_LK_Rubro">
      <DTS:VariableValue
        DTS:DataType="8">UPDATE A SET
       A.[SK_ACE_Codigo] = CASE WHEN A.[Name] = B.[CD_ID_Origen] THEN B.SK_ACE_Codigo ELSE 0 END
FROM [STG].[dbo].[STG_LK_Rubro] A
INNER JOIN [DWH_Test].[dbo].[LK_ActividadEconomica] B ON A.[Name] = B.[CD_ID_Origen]

;

