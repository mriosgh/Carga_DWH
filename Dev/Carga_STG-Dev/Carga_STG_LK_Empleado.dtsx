                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT Distinct (ABAN8) as ABAN8,	
	(ABALKY) as ABALKY,	
	(ABTAX) as ABTAX,	
	(ABALPH) as ABALPH,
	(ABDC) as ABDC,
	'mail'= (
		 CASE
		 WHEN cast(F0101.ABAN8 as int)=402228 THEN 'ngomis@sullair.com.ar'
		 WHEN cast(F0101.ABAN8 as int)=402242 THEN 'evergel@sullair.com.ar'
		 WHEN cast(F0101.ABAN8 as int)=402276 THEN 'JaDiaz@sullair.com.ar'
		 WHEN cast(F0101.ABAN8 as int)=402291 THEN 'jvendrell@sullair.com.ar'
		 WHEN cast(F0101.ABAN8 as int)=402127 THEN 'gsacson@sullair.com.ar'
		 WHEN cast(F0101.ABAN8 as int)=402191 THEN 'pamsuarez@sullair.com.ar'
		 WHEN cast(F0101.ABAN8 as int)=402195 THEN 'cprimo@sullair.com.ar'
		 WHEN cast(F0101.ABAN8 as int)=402227 THEN 'mformica@sullair.com.ar'
		 WHEN cast(F0101.ABAN8 as int)=402265 THEN 'ncensabella@sullair.com.ar'
		 WHEN cast(F0101.ABAN8 as int)=402299 THEN 'gmavric@sullair.com.ar'
                                      WHEN cast(F0101.ABAN8 as int)=401865 THEN 'eaguirre@sullair.com.ar'
                                      WHEN cast(F0101.ABAN8 as int)=402189 THEN 'mfarias@sullair.com.ar'
		WHEN u.WWREM1 IS NULL THEN '*sin dato*'
		 ELSE LTRIM(RTRIM(u.WWREM1)) END ),
	concat(CAST (  DateAdd (d, cast(ABUPMJ   as int) - ((cast(ABUPMJ as int) / 1000) * 1000), 
	convert(datetime, str((cast(ABUPMJ as int) / 1000) + 1900 ) + '0101')) -1  AS DATE),
	' ',substring(right ('000000'+ cast( ABUPMT as varchar(6)),6), 1, 2 )+':'+
	substring(right ('000000'+ cast( ABUPMT as varchar(6)),6), 3, 2 )+':'+
	substring(right ('000000'+ cast( ABUPMT as varchar(6)),6), 5, 2 ))FECHAHORA
FROM [JDE_REP].[PRODDTA].[F0101]  
 LEFT JOIN [JDE_REP].[PRODDTA].[F0111]  as u
  ON u.WWAN8=F0101.ABAN8
where ABAT1 = 'E'</property>
