              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Input JDE_Provision"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT	CASE
		WHEN LEN(P.IDrubro)&gt;1 THEN cast(LEFT(P.IDSucursal,2)+(RIGHT(P.IDRubro,1)) as bigint) 
		ELSE cast(LEFT(P.IDSucursal,2)+P.IDRubro as bigint) 
		END AS Unidad
		,IDClienteFC 
		,LTRIM(RTRIM(IDArticulo)) AS IDArticulo
		,CatVta3
		,LTRIM(RTRIM(Interno)) AS Interno
		,CntDias
		,iif(IDMonedaFC=1,AFacturarFC,AFacturarFC*TCMES) AS AFacturarFC
		,InicioFac as InicioPeriodo
		,CorteFac as  FinPeriodo
		,CASE
		WHEN MesProvision= 1 THEN 'Provisión Enero'
		WHEN  MesProvision= 2 THEN 'Provisión Febrero'
		WHEN  MesProvision= 3 THEN 'Provisión Marzo'
		WHEN  MesProvision= 4 THEN 'Provisión Abril'
		WHEN  MesProvision= 5 THEN 'Provisión Mayo'
		WHEN  MesProvision= 6 THEN 'Provisión Junio'
		WHEN  MesProvision= 7 THEN 'Provisión Julio'
		WHEN  MesProvision= 8 THEN 'Provisión Agosto'
		WHEN  MesProvision= 9 THEN 'Provisión Septiembre'
		WHEN  MesProvision= 10 THEN 'Provisión Octubre'
		WHEN  MesProvision= 11 THEN 'Provisión Noviembre'
		WHEN  MesProvision= 12 THEN 'Provisión Diciembre'
		END AS Mes_Provision
                                     ,dateadd(d,1,EOMONTH(getdate(),-2)) as FechaProvision
		,[AnoProvision] AS Año
		,LTRIM(RTRIM(S.Descripcion)) AS Segmento
		,LTRIM(RTRIM(A.Descripcion)) AS Actividad
		,LTRIM(RTRIM(C.Nombre)) AS Comercial
		,IDFacturacion
		,P.IDActividad as JDE_IDActividad
		,P.IDSegmento as JDE_IDSegmento
		,cast(c.IDComercial as int ) AS idComercial
		,Cast(Interno as nvarchar(255))as JDE_Interno
		,P.IDDeposito as JDE_IDDeposito
FROM [dbo].[Provision] P
INNER JOIN [dbo].[TT_SegmentosComercial] S
ON P.IDSegmento = S.IDSegmento
INNER JOIN [dbo].[TT_ActividadComercial] A
ON P.IDActividad = A.IDActividad
INNER JOIN [dbo].[TT_Comerciales] C
ON P.IDComercialNOE= C.IDComercial
WHERE P.Estado in (1,3) and MesProvision =?
AND [AnoProvision] = ?</property>
