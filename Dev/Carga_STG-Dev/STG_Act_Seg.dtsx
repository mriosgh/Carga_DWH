            <component
              refId="Package\Carga STG_Act_Seg\QueryDev"
              componentClassID="Microsoft.Lookup"
              contactInfo="Lookup;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;6"
              description="Joins additional columns to the data flow by looking up values in a table. For example, join to the 'employee id' column the employees table to get 'hire date' and 'employee name'. We recommend this transformation when the lookup table can fit into memory."
              name="QueryDev"
              usesDispositions="true"
              version="6">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement that generates the lookup table."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select

f.IDFacturacion,

CASE
WHEN f.IDIade = 0 or f.IDIade is null  
THEN N.IDSegmento

ELSE sa.IDSeg

END AS JDE_idSegmento,

CASE
WHEN f.IDIade = 0 or f.IDIade is null  
THEN N.IDActividad

ELSE sa.IDAct
END AS JDE_idActividad,

iif(UPPER(LTRIM(RTRIM(cast(U.Email AS NVARCHAR (50))))) is null, UPPER(LTRIM(RTRIM(cast(sa.Email AS NVARCHAR (50))))), UPPER(LTRIM(RTRIM(cast(U.Email AS NVARCHAR (50)))))) AS Email

from Facturacion f

left join IADE i
on i.IDIade = f.IDIade

LEFT JOIN  [dbo].[Noes] N

ON F.IDNoe = N.IDNoe
LEFT JOIN  [dbo].[Usuarios] U

ON U.IDComercial = N.IDComercial
  
AND U.IDComercial &lt;&gt; 0

LEFT JOIN [dbo].[Remitos] R

ON R.IDRto = F.IDRto

Left join (

select  
i.IDIade,
max(r.FechaReal) as fechaUltRto

from IADE i

inner join Remitos r
  on r.FechaReal&lt;= i.Fecha

and r.tipo = 'DEV'

inner join RemitosDetalle rd 
on r.IDRto = rd.IDRto
 
and rd.Interno = i.Interno
group by i.IDIade) ir

on i.IDIade = ir.IDIade
left join (
select Interno, FechaReal, r.IDCliente, max(IDSegmento) as IDSeg, max(IDActividad) as IDAct,r.IDDeposito,u.Email

from Remitos r 

inner join RemitosDetalle  rd

on r.IDRto = rd.IDRto
   and r.tipo = 'DEV'
   and rd.interno != ''

inner join Noes n 
on n.IDNoe = rd.IDNoe
 
left join Usuarios u
 on u.IDComercial = n.idcomercial 
group by Interno, FechaReal, r.IDCliente, r.IDDeposito,u.Email ) sa
 
on sa.Interno = i.Interno
 and sa.FechaReal = ir.fechaUltRto 

and sa.IDCliente = f.IDCliente</property>
