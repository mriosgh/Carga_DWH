              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
f.IDFacturacion,
CASE 
	WHEN f.IDIade = 0 or f.IDIade is null  THEN N.IDSegmento
	ELSE sa.IDSeg 
	END AS JDE_idSegmento, 
CASE 
	WHEN f.IDIade = 0 or f.IDIade is null  THEN N.IDActividad
	ELSE sa.IDAct 
	END AS JDE_idActividad,
iif(UPPER(LTRIM(RTRIM(cast(U.Email AS NVARCHAR (50))))) is null, UPPER(LTRIM(RTRIM(cast(sa.Email AS NVARCHAR (50))))), UPPER(LTRIM(RTRIM(cast(U.Email AS NVARCHAR (50)))))) AS Email
from Facturacion f
left join IADE i
on i.IDIade = f.IDIade
LEFT JOIN  [dbo].[Noes] N
	ON F.IDNoe = N.IDNoe
LEFT JOIN  [dbo].[Usuarios] U
	ON U.IDComercial = N.IDComercial 
	AND U.IDComercial &lt;&gt; 0
	LEFT JOIN [dbo].[Remitos] R
	ON R.IDRto = F.IDRto
Left join (
			select	
				i.IDIade, 
				max(r.FechaReal) as fechaUltRto
				from IADE i
				inner join Remitos r
				on r.FechaReal&lt;= i.Fecha --{Fecha creación de IADE}
				inner join RemitosDetalle rd
				on r.IDRto = rd.IDRto
				and rd.Interno = i.Interno
				group by i.IDIade) ir
on i.IDIade = ir.IDIade
left join (
	select Interno, FechaReal, r.IDCliente, max(IDSegmento) as IDSeg, max(IDActividad) as IDAct,r.IDDeposito,u.Email
		from Remitos r
		inner join RemitosDetalle  rd
		on r.IDRto = rd.IDRto
		and rd.interno != ''
		inner join Noes n
		on n.IDNoe = rd.IDNoe
		left join Usuarios u
		on u.IDComercial = n.idcomercial
		group by Interno, FechaReal, r.IDCliente, r.IDDeposito,u.Email ) sa
on sa.Interno = i.Interno
and sa.FechaReal = ir.fechaUltRto
and sa.IDCliente = f.IDCliente</property>
