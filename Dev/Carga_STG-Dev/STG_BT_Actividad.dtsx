                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT F.SK_MES,
FC_Fecha, 
SK_Trimestre, 
SK_Semana, 
SK_Bimestre, 
SK_Año, 
SK_DiaSemana, 
SK_MesAnterior, 
FC_PrimerDia, 
FC_UltimoDia
FROM DBO.LK_Fecha F WITH(NOLOCK)
INNER JOIN DBO.LK_Mes M WITH(NOLOCK)
ON F.SK_MES = M.SK_Mes
WHERE SK_Trimestre = (
						SELECT SK_Trimestre
						FROM DBO.LK_Fecha F WITH(NOLOCK)
						INNER JOIN DBO.LK_Mes M WITH(NOLOCK)
						ON F.SK_MES = M.SK_Mes
						WHERE CONVERT(VARCHAR(12),FC_Fecha,112) = CONVERT(VARCHAR(12),GETDATE(),112))</property>
