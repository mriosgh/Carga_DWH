              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
CAST(cab.IdProveedor AS INT) AS PHAN8
,rtrim(ltrim (CodigoDeInforme2Compras)) as PDPDP2
,CASE WHEN cab.UnidadDeNegocio IN ('901BA', '901B') AND CodigoDeInforme2Compras = 'P' THEN 'FAB' ELSE RTRIM(LTRIM(CodigoDeInforme1Compras)) END AS PDPDP1
,RTRIM(LTRIM(cab.UnidadDeNegocio)) AS PHMCU
,cab.Compania as PHKCOO
,cab.NroOrdenCompra as PHDOCO
,cab.TipoDeOrden as PHDCTO
,cab.SufijoOrdenCompra as PHSFXO
,NroDeLinea as PDLNID
,CodigoDeEstadoultimo as PDLTTR
,CodigoDeEstadoSiguiente as PDNXTR
,cab.CodigoDeMonedaDesde as PHCRCD 
,cab.ValorAbiertoExtranjero as PHFAP
,DET.ValorTotalExtranjero as PDFEA 
,cab.CantidadPedidoBruto as PHOTOT  
,det.ValorPrecioTotal as PDAEXP 
,det.CodigoDeMonedaDesde as PDCRCD 
,cab.FechaPedidoTransaccion as PHTRDJ 
,det.FechaActualizacion as PDUPMJ

FROM DBO.STG_OrdenCompraCabecera CAB WITH(NOLOCK)
INNER JOIN DBO.STG_OrdenCompraLineas DET WITH(NOLOCK)
	ON CAB.Compania =  DET.Compania
	AND CAB.NroOrdenCompra = det.NroOrdenCompra
	AND CAB.TipoDeOrden = DET.TipoDeOrden
	AND CAB.SufijoOrdenCompra = DET.SufijoOrdenCompra
WHERE cab.TipoDeOrden IN ('OC','OI')
AND ( cab.FechaPedidoTransaccion  IN (SELECT FC_FECHA FROM [dbo].[STG_TrimestreActual])
OR det.FechaActualizacion  IN (SELECT FC_FECHA FROM [dbo].[STG_TrimestreActual]))
ORDER BY cab.Compania, cab.NroOrdenCompra, cab.TipoDeOrden, cab.SufijoOrdenCompra, NroDeLinea</property>
